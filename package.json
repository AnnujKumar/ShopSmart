{
  "name": "ecommerce-app",
  "version": "1.0.0",
  "description": "E-Commerce Application with React Frontend and Express Backend",
  "main": "index.js",
  "scripts": {
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm run dev",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "seed": "node seed.js",
    "setup": "npm run seed && npm start"
  },
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
